{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/components/*": ["src/components/*"],
			"@/layouts/*": ["src/layouts/*"],
			"@/utils/*": ["src/utils/*"],
			"@/data/*": ["src/data/*"],
			"@/site-config": ["src/data/site-config.ts"],
			"@/styles/*": ["src/styles/*"]
		}
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist", ".vscode"]
}
